<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>RServlet</servlet-name>
        <servlet-class>MainPage.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RServlet</servlet-name>
        <url-pattern>/nmsl</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LServlet</servlet-name>
        <servlet-class>MainPage.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GroupSpaceServlet</servlet-name>
        <servlet-class>MainPage.GroupSpaceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GroupSpaceServlet</servlet-name>
        <url-pattern>/GroupSpace</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MySpaceServlet</servlet-name>
        <servlet-class>MainPage.MySpaceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MySpaceServlet</servlet-name>
        <url-pattern>/MySpace</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ShareSpace0Servlet</servlet-name>
        <servlet-class>MainPage.ShareSpace0Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShareSpace0Servlet</servlet-name>
        <url-pattern>/ShareSpace0</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoginOutServlet</servlet-name>
        <servlet-class>MainPage.LoginOutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginOutServlet</servlet-name>
        <url-pattern>/LoginOut</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>fileListServlet</servlet-name>
        <servlet-class>MainPage.FileListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>fileListServlet</servlet-name>
        <url-pattern>/data</url-pattern>
    </servlet-mapping>
</web-app>
